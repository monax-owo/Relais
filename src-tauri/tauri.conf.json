{
  "productName": "Relais",
  "version": "0.1.1",
  "identifier": "io.github.monax-owo.relais",
  "build": {
    "beforeBuildCommand": "pnpm run vite:build",
    "beforeDevCommand": "pnpm run vite:dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": ["msi", "nsis"]
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
